{
  "name": "@omrilotan/dotfiles",
  "version": "0.0.0",
  "description": "Install global node modules here to be available across all node versions",
  "private": true,
  "scripts": {
    "clean": "rm -rf /usr/local/lib/node_modules",
    "format": "prettier --write .",
    "update": "./.scripts/update.js",
    "postinstall": "./.scripts/latest.js"
  },
  "dependencies": {
    "@githubnext/github-copilot-cli": "latest",
    "@nestjs/cli": "latest",
    "async-execute": "latest",
    "committee": "latest",
    "http-server": "latest",
    "localtunnel": "latest",
    "ngrok": "latest",
    "npm": "latest",
    "npq": "latest",
    "typescript": "latest",
    "unimported": "latest",
    "web-ext": "latest",
    "workbox-cli": "latest",
    "wrangler": "latest",
    "yarn": "latest"
  },
  "devDependencies": {
    "prettier": "^3.3.3"
  }
}
